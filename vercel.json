{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://geomancy-blog.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/index.html"
    }
  ]
}
